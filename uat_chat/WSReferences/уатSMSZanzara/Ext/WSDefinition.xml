				</s:complexType>
			</s:element>
			<s:complexType name="Z_result">
				<s:attribute name="res"
						type="s:int"
						use="required"/>
				<s:attribute name="description"
						type="s:string"/>
			</s:complexType>
			<s:element name="sms_send">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="unbounded"
								name="sms"
								type="xsd1:Z_sendSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_sendSms">
				<s:sequence>
					<s:element minOccurs="0"
							maxOccurs="1"
							name="message"
							type="s:string"/>
				</s:sequence>
				<s:attribute name="sms_id"
						type="s:string"/>
				<s:attribute name="source_number"
						type="s:string"/>
				<s:attribute name="number"
						type="s:string"/>
				<s:attribute name="ttl"
						type="s:long"/>
				<s:attribute name="schedule_delivery_time"
						type="s:dateTime"/>
				<s:attribute name="port"
						type="s:int"/>
				<s:attribute name="email"
						type="s:string"/>
			</s:complexType>
			<s:element name="sms_sendResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="sms_sendResult"
								type="xsd1:Z_result_sendSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_result_sendSms"
					mixed="false">
				<s:complexContent mixed="false">
					<s:extension base="xsd1:Z_result">
						<s:sequence>
							<s:element minOccurs="0"
									maxOccurs="unbounded"
									name="sendres"
									type="xsd1:Z_resultPush"/>
						</s:sequence>
					</s:extension>
				</s:complexContent>
			</s:complexType>
			<s:complexType name="Z_resultPush">
				<s:attribute name="sms_id"
						type="s:string"/>
				<s:attribute name="push_id"
						type="s:long"/>
				<s:attribute name="description"
						type="s:string"/>
				<s:attribute name="res"
						type="s:int"
						use="required"/>
				<s:attribute name="sms_count"
						type="s:int"/>
			</s:complexType>
			<s:element name="sms_send_common">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="unbounded"
								name="sms"
								type="xsd1:Z_sendSms"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="common"
								type="xsd1:Z_commonParam"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_commonParam">
				<s:sequence>
					<s:element minOccurs="0"
							maxOccurs="1"
							name="message"
							type="s:string"/>
				</s:sequence>
				<s:attribute name="source_number"
						type="s:string"/>
				<s:attribute name="ttl"
						type="s:long"/>
				<s:attribute name="schedule_delivery_time"
						type="s:dateTime"/>
				<s:attribute name="port"
						type="s:int"/>
			</s:complexType>
			<s:element name="sms_send_commonResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="sms_send_commonResult"
								type="xsd1:Z_result_sendSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="VCARD_send">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="unbounded"
								name="sms"
								type="xsd1:Z_sendSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="VCARD_sendResponse">
