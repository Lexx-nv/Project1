						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_UserInfo">
				<s:attribute name="usr"
						type="s:string"/>
				<s:attribute name="pwd"
						type="s:string"/>
				<s:attribute name="appinfo"
						type="s:string"/>
				<s:attribute name="pv"
						type="s:int"/>
			</s:complexType>
			<s:element name="transport_stateResponse">
