								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="unbounded"
								name="sms"
								type="xsd1:Z_sendSms"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="common"
								type="xsd1:Z_commonParam"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="VCARD_send_commonResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="VCARD_send_commonResult"
								type="xsd1:Z_result_sendSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="sms_status">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="unbounded"
								name="status"
								type="xsd1:Z_statusSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_statusSms">
				<s:attribute name="push_id"
						type="s:long"
						use="required"/>
			</s:complexType>
			<s:element name="sms_statusResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="sms_statusResult"
								type="xsd1:Z_result_statusSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_result_statusSms"
					mixed="false">
				<s:complexContent mixed="false">
					<s:extension base="xsd1:Z_result">
						<s:sequence>
							<s:element minOccurs="0"
									maxOccurs="unbounded"
									name="statusres"
									type="xsd1:Z_resultStatus"/>
						</s:sequence>
					</s:extension>
				</s:complexContent>
			</s:complexType>
			<s:complexType name="Z_resultStatus">
				<s:attribute name="push_id"
						type="s:long"
						use="required"/>
				<s:attribute name="description"
						type="s:string"/>
				<s:attribute name="status"
						type="s:int"
						use="required"/>
				<s:attribute name="statustime"
						type="s:dateTime"
						use="required"/>
				<s:attribute name="SMS_sent"
						type="s:int"
						use="required"/>
				<s:attribute name="SMS_dlvr"
						type="s:int"
						use="required"/>
			</s:complexType>
			<s:element name="sms_receive">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="req_sms"
								type="xsd1:Z_receiveSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_receiveSms">
				<s:attribute name="request_id"
						type="s:string"/>
				<s:attribute name="count"
						type="s:int"/>
			</s:complexType>
			<s:element name="sms_receiveResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="sms_receiveResult"
								type="xsd1:Z_result_receiveSms"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_result_receiveSms"
					mixed="false">
				<s:complexContent mixed="false">
					<s:extension base="xsd1:Z_result">
						<s:sequence>
							<s:element minOccurs="0"
									maxOccurs="unbounded"
									name="receive"
									type="xsd1:Z_resultreceiveSms"/>
						</s:sequence>
						<s:attribute name="sms_count"
								type="s:int"
								use="required"/>
						<s:attribute name="sms_remains"
								type="s:int"
								use="required"/>
						<s:attribute name="request_id"
								type="s:string"/>
					</s:extension>
				</s:complexContent>
			</s:complexType>
			<s:complexType name="Z_resultreceiveSms">
				<s:attribute name="response_id"
						type="s:long"
						use="required"/>
				<s:attribute name="src_number"
						type="s:string"/>
				<s:attribute name="number"
						type="s:string"/>
				<s:attribute name="text"
						type="s:string"/>
				<s:attribute name="request_time"
						type="s:dateTime"
						use="required"/>
			</s:complexType>
			<s:element name="get_sourceaddress">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="get_sourceaddressResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="get_sourceaddressResult"
								type="xsd1:Z_result_SourceAddress"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_result_SourceAddress"
					mixed="false">
				<s:complexContent mixed="false">
					<s:extension base="xsd1:Z_result">
						<s:sequence>
							<s:element minOccurs="0"
									maxOccurs="unbounded"
									name="sourceaddress"
									type="s:string"/>
						</s:sequence>
					</s:extension>
				</s:complexContent>
			</s:complexType>
			<s:element name="get_statictic">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="userinfo"
								type="xsd1:Z_UserInfo"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="source_address"
								type="s:string"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="category"
								type="s:string"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="beg_time"
								type="s:string"/>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="end_time"
								type="s:string"/>
						<s:element minOccurs="1"
								maxOccurs="1"
								name="group_by_time"
								type="s:int"/>
						<s:element minOccurs="1"
								maxOccurs="1"
								name="group_by_source"
								type="s:boolean"/>
						<s:element minOccurs="1"
								maxOccurs="1"
								name="divide_by_category"
								type="s:boolean"/>
						<s:element minOccurs="1"
								maxOccurs="1"
								name="sub_client"
								type="s:boolean"/>
						<s:element minOccurs="1"
								maxOccurs="1"
								name="only_top_client"
								type="s:boolean"/>
						<s:element minOccurs="1"
								maxOccurs="1"
								name="show_empty"
								type="s:boolean"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="get_staticticResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="get_staticticResult"
								type="xsd1:Z_result_statistic"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:complexType name="Z_result_statistic"
					mixed="false">
				<s:complexContent mixed="false">
					<s:extension base="xsd1:Z_result">
						<s:sequence>
							<s:element minOccurs="0"
									maxOccurs="unbounded"
									name="stat"
									type="xsd1:Z_statistic_item"/>
						</s:sequence>
					</s:extension>
				</s:complexContent>
			</s:complexType>
			<s:complexType name="Z_statistic_item">
				<s:attribute name="dealer_name"
						type="s:string"/>
				<s:attribute name="client_name"
						type="s:string"/>
				<s:attribute name="src_addr"
						type="s:string"/>
				<s:attribute name="snd_cnt"
						type="s:int"
						use="required"/>
				<s:attribute name="dlv_cnt"
						type="s:int"
						use="required"/>
				<s:attribute name="rcv_cnt"
						type="s:int"
						use="required"/>
				<s:attribute name="beg_time"
						type="s:string"/>
				<s:attribute name="end_time"
						type="s:string"/>
				<s:attribute name="category"
						type="s:string"/>
				<s:attribute name="dealer_id"
						type="s:int"
						use="required"/>
				<s:attribute name="client_id"
						type="s:int"
						use="required"/>
				<s:attribute name="is_dealer"
						type="s:int"
						use="required"/>
				<s:attribute name="client_status"
						type="s:int"
						use="required"/>
				<s:attribute name="source_status"
						type="s:int"
						use="required"/>
			</s:complexType>
		</s:schema>
	</types>
	<message name="HelloWorldSoapIn">
		<part name="parameters"
				element="tns:HelloWorld"/>
	</message>
	<message name="HelloWorldSoapOut">
		<part name="parameters"
				element="tns:HelloWorldResponse"/>
	</message>
	<message name="transport_state_appSoapIn">
		<part name="parameters"
				element="tns:transport_state_app"/>
	</message>
	<message name="transport_state_appSoapOut">
		<part name="parameters"
				element="tns:transport_state_appResponse"/>
	</message>
	<message name="transport_stateSoapIn">
		<part name="parameters"
				element="tns:transport_state"/>
	</message>
	<message name="transport_stateSoapOut">
		<part name="parameters"
				element="tns:transport_stateResponse"/>
	</message>
	<message name="sms_sendSoapIn">
