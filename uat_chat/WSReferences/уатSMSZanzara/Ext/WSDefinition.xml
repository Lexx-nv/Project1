			<s:element name="HelloWorld">
				<s:complexType/>
			</s:element>
			<s:element name="HelloWorldResponse">
				<s:complexType>
					<s:sequence>
						<s:element minOccurs="0"
								maxOccurs="1"
								name="HelloWorldResult"
								type="s:string"/>
					</s:sequence>
				</s:complexType>
			</s:element>
			<s:element name="transport_state_app">
