		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	РаботаВодителей.ДатаРаботы,
	ВЫБОР
		КОГДА РаботаВодителей.КПутевомуЛисту = ЗНАЧЕНИЕ(Документ.уатПутевойЛист.ПустаяСсылка)
			ТОГДА РаботаВодителей.Регистратор
		ИНАЧЕ РаботаВодителей.КПутевомуЛисту
	КОНЕЦ КАК Регистратор,
	РаботаВодителей.Водитель,
	РаботаВодителей.ВидРасчета,
	РаботаВодителей.ПериодРаботы,
	РаботаВодителей.ТС,
	СУММА(РаботаВодителей.Количество) КАК Количество,
	СУММА(ВЫБОР
			КОГДА РаботаВодителей.ВидРасчета.ЭтоРабочиеЧасы
				ТОГДА РаботаВодителей.Количество
			ИНАЧЕ 0
		КОНЕЦ) КАК КоличествоОсн,
	СУММА(РаботаВодителей.Сумма) КАК Сумма,
	
	CASE WHEN СУММА(РаботаВодителей.ФактЧасовПоСделке)=0 THEN 0
	ELSE СУММА(РаботаВодителей.НормаЧасовПоСделке)/СУММА(РаботаВодителей.ФактЧасовПоСделке)*100 END КАК ПроцСделка,
	
	РаботаВодителей.ТС.ГаражныйНомер,
	РаботаВодителей.ТС.ГосударственныйНомер,
	тбТрф.Тариф КАК Тариф,
	CASE WHEN ISNULL(тбОкл.ЭтоОклад,false) THEN 1 ELSE 0 END ЭтоОклад
ИЗ
	РегистрНакопления.РаботаВодителей КАК РаботаВодителей
	INNER JOIN (
			SELECT 
				РаботаВодителей.ДатаРаботы,
				МАКСИМУМ(РаботаВодителей.Тариф) КАК Тариф 	
			FROM  РегистрНакопления.РаботаВодителей КАК РаботаВодителей
			WHERE РаботаВодителей.Период &gt;= &amp;Дт
				И РаботаВодителей.Период &lt; &amp;дт1
    			и Водитель = &amp;Сотр
			GROUP BY ДатаРаботы
				)  тбТрф ON тбТрф.ДатаРаботы = РаботаВодителей.ДатаРаботы
				
	LEFT OUTER JOIN РегистрСведений.уатТарифыСотрудников.СрезПоследних(&amp;Дт1,Сотрудник = &amp;Сотр) тбОкл ON тбОкл.Сотрудник = РаботаВодителей.Водитель
				
				
ГДЕ
	РаботаВодителей.Период &gt;= &amp;Дт
	И РаботаВодителей.Период &lt; &amp;дт1
    и Водитель = &amp;Сотр

СГРУППИРОВАТЬ ПО
	РаботаВодителей.ДатаРаботы,
	ВЫБОР
		КОГДА РаботаВодителей.КПутевомуЛисту = ЗНАЧЕНИЕ(Документ.уатПутевойЛист.ПустаяСсылка)
			ТОГДА РаботаВодителей.Регистратор
		ИНАЧЕ РаботаВодителей.КПутевомуЛисту
	КОНЕЦ,
	РаботаВодителей.Водитель,
	РаботаВодителей.ВидРасчета,
	РаботаВодителей.ПериодРаботы,
	РаботаВодителей.ТС,
	РаботаВодителей.ТС.ГаражныйНомер,
	РаботаВодителей.ТС.ГосударственныйНомер,
	тбТрф.Тариф,
	тбОкл.ЭтоОклад</query>
