		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	уатМестонахождениеТССрезПоследних.ТС,
	уатМестонахождениеТССрезПоследних.Подразделение,
	уатМестонахождениеТССрезПоследних.Период,
	уатМестонахождениеТССрезПоследних.Состояние
ПОМЕСТИТЬ ВРТбл
ИЗ
	РегистрСведений.уатМестонахождениеТС.СрезПоследних(&amp;Дт, ) КАК уатМестонахождениеТССрезПоследних

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	уатМестонахождениеТС.ТС,
	уатМестонахождениеТС.Подразделение,
	уатМестонахождениеТС.Период,
	уатМестонахождениеТС.Состояние
ИЗ
	РегистрСведений.уатМестонахождениеТС КАК уатМестонахождениеТС
ГДЕ
	уатМестонахождениеТС.Период &gt; &amp;Дт
	И уатМестонахождениеТС.Период &lt; &amp;Дт1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Тбл.ТС,
	Тбл.Подразделение,
	Тбл.Период,
	МИНИМУМ(ЕСТЬNULL(Т.Период, &amp;Дт1)) КАК ПериодКон,
	Тбл.Период КАК изначДт,
	Тбл.Состояние
ПОМЕСТИТЬ ВРТбл1
ИЗ
	ВРТбл КАК Тбл
		ЛЕВОЕ СОЕДИНЕНИЕ ВРТбл КАК Т
		ПО (Т.ТС = Тбл.ТС)
			И (Т.Период &gt; Тбл.Период)

СГРУППИРОВАТЬ ПО
	Тбл.ТС,
	Тбл.Подразделение,
	Тбл.Период,
	Тбл.Состояние,
	Тбл.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Тбл.ТС,
	Тбл.Подразделение,
	Тбл.Период,
	Тбл.ПериодКон,
	Тбл.Состояние
ПОМЕСТИТЬ ВРПодр
ИЗ
	ВРТбл1 КАК Тбл
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			Т1.ТС КАК ТС,
			Т1.Период КАК Период,
			МАКСИМУМ(Т1.изначДт) КАК изНачДт
		ИЗ
			ВРТбл1 КАК Т1
		
		СГРУППИРОВАТЬ ПО
			Т1.ТС,
			Т1.Период) КАК Т
		ПО (Т.ТС = Тбл.ТС)
			И (Т.Период = Тбл.Период)
			И (Т.изНачДт = Тбл.изначДт)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	уатПутевойЛистЗадание.Ссылка,
	уатПутевойЛистЗадание.КоличествоКмПорожний,
	уатПутевойЛистЗадание.КоличествоКмСпец5 КАК ПробегРазница,
	уатПутевойЛистЗадание.ИдСтроки
ПОМЕСТИТЬ ВрПробегИзПутевых
ИЗ
	Документ.уатПутевойЛист.Задание КАК уатПутевойЛистЗадание
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	регРее.ПутевойЛист,
	регРее.ИдСтроки
ПОМЕСТИТЬ врРее
ИЗ
	РегистрНакопления.РаботаТС КАК регРее
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.уатПутевойЛист КАК Док
		ПО (Док.Ссылка = регРее.ПутевойЛист)
			И (регРее.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход))
			И (Док.Дата &gt;= &amp;Дт)
			И (Док.Дата &lt; &amp;Дт1)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫБОР
		КОГДА уатРаботаТС.Контрагент ССЫЛКА Справочник.Контрагенты
			ТОГДА "Внешний заказчик"
		ИНАЧЕ "Внутренний заказчик"
	КОНЕЦ КАК ВидЗаказчика,
	ВЫБОР
		КОГДА уатРаботаТС.Контрагент ССЫЛКА Справочник.Контрагенты
			ТОГДА уатРаботаТС.Контрагент
		ИНАЧЕ уатРаботаТС.Подразделение
	КОНЕЦ КАК Контрагент,
	ВЫБОР
		КОГДА уатРаботаТС.Контрагент ССЫЛКА Справочник.Контрагенты
			ТОГДА ЗНАЧЕНИЕ(Справочник.ВидыДеятельностиКонтрагентов.ПустаяСсылка)
		ИНАЧЕ уатРаботаТС.Объект.ВидДеятельности
	КОНЕЦ КАК ВидДеятельности,
	ВЫБОР
		КОГДА уатРаботаТС.Контрагент ССЫЛКА Справочник.Контрагенты
			ТОГДА уатРаботаТС.ЦехКонтрагента.Родитель
		ИНАЧЕ уатРаботаТС.Объект.Родитель
	КОНЕЦ КАК Родитель,
	ВЫБОР
		КОГДА уатРаботаТС.Контрагент ССЫЛКА Справочник.Контрагенты
			ТОГДА уатРаботаТС.ЦехКонтрагента
		ИНАЧЕ уатРаботаТС.Объект
	КОНЕЦ КАК ЦехКонтрагента,
	ВЫБОР
		КОГДА тбПодр.Подразделение = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА "!!!Подразделение у ТС на указано!!!"
		ИНАЧЕ тбПодр.Подразделение
	КОНЕЦ КАК Колонна,
	уатРаботаТС.ТС КАК ТС,
	уатРаботаТС.ТС.ТипТС КАК типТС,
	уатРаботаТС.ПутевойЛист КАК Регистратор,
	НАЧАЛОПЕРИОДА(уатРаботаТС.Период, ДЕНЬ) КАК ДтДень,
	уатРаботаТС.ТС.ГаражныйНомер КАК ТСГаражныйНомер,
	уатРаботаТС.ТС.ГосударственныйНомер КАК ТСГосударственныйНомер,
	уатРаботаТС.Номенклатура,
	уатРаботаТС.Реестр.АдресОтправления КАК ПунктОтправки,
	уатРаботаТС.Реестр.АдресДоставки КАК ПунктДоставки,
	уатРаботаТС.Реестр.СпособПогрузки КАК СпособПогрузки,
	уатРаботаТС.Груз,
	уатРаботаТС.ГруппаДорог,
	ЕСТЬNULL(регРее.ПутевойЛист, уатРаботаТС.Реестр) КАК Реестр,
	ВЫБОР
		КОГДА уатРаботаТС.Реестр.Номер ЕСТЬ NULL
			ТОГДА "Повременная"
		ИНАЧЕ "Сдельная"
	КОНЕЦ КАК ВидРаботы,
	ВЫБОР
		КОГДА уатРаботаТС.ТС.Модель.НаличиеСпидометра = ЛОЖЬ
			ТОГДА 0
		ИНАЧЕ уатРаботаТС.Пробег
	КОНЕЦ КАК Пробег,
	ВЫБОР
		КОГДА уатРаботаТС.ТС.Модель.НаличиеСпидометра = ЛОЖЬ
			ТОГДА уатРаботаТС.Пробег
		ИНАЧЕ 0
	КОНЕЦ КАК ПробегМЧ,
	ВЫБОР
		КОГДА уатРаботаТС.ПробегСГрузом &lt;&gt; 0
			ТОГДА уатРаботаТС.Пробег - уатРаботаТС.ПробегСГрузом
		ИНАЧЕ 0
	КОНЕЦ КАК ПробегПустой,
	уатРаботаТС.ПробегСГрузом,
	уатРаботаТС.Объем,
	уатРаботаТС.Тонны,
	уатРаботаТС.ТнКм,
	уатРаботаТС.Сумма + уатРаботаТС.Сумма1 + уатРаботаТС.Сумма2 + уатРаботаТС.Сумма3 + уатРаботаТС.Сумма4 КАК Сумма,
	ВрПробегИзПутевых.ПробегРазница,
	уатРаботаТС.КоличествоЧасов + ВЫБОР
		КОГДА уатРаботаТС.Прейскурант.Услуга1.Наименование ПОДОБНО "%Отстой%"
			ТОГДА уатРаботаТС.Услуга1
		ИНАЧЕ 0
	КОНЕЦ КАК Количество,
	ВЫБОР
		КОГДА ЕСТЬNULL(тбПодр.Состояние.ЭтоПривленный, ИСТИНА) = ЛОЖЬ
				И ЕСТЬNULL(уатРаботаТС.ТС.ТипТС.НеВыводитьВТабельТС, ЛОЖЬ) = ИСТИНА
			ТОГДА уатРаботаТС.КоличествоЧасов + ВЫБОР
					КОГДА уатРаботаТС.Прейскурант.Услуга1.Наименование ПОДОБНО "%Отстой%"
						ТОГДА уатРаботаТС.Услуга1
					ИНАЧЕ 0
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК ЧасыНеВТабеле,
	ВЫБОР
		КОГДА тбПодр.Состояние = ЗНАЧЕНИЕ(Справочник.уатСостояниеТС.Привлеченный)
				ИЛИ тбПодр.Состояние ЕСТЬ NULL
				ИЛИ тбПодр.Состояние.ЭтоПривленный = ИСТИНА
			ТОГДА уатРаботаТС.КоличествоЧасов + ВЫБОР
					КОГДА уатРаботаТС.Прейскурант.Услуга1.Наименование ПОДОБНО "%Отстой%"
						ТОГДА уатРаботаТС.Услуга1
					ИНАЧЕ 0
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК ЧасыПривлеченных,
	ВЫБОР
		КОГДА ЕСТЬNULL(тбПодр.Состояние.ЭтоПривленный, ИСТИНА) = ЛОЖЬ
				И ЕСТЬNULL(уатРаботаТС.ТС.ТипТС.НеВыводитьВТабельТС, ЛОЖЬ) = ЛОЖЬ
			ТОГДА уатРаботаТС.КоличествоЧасов + ВЫБОР
					КОГДА уатРаботаТС.Прейскурант.Услуга1.Наименование ПОДОБНО "%Отстой%"
						ТОГДА уатРаботаТС.Услуга1
					ИНАЧЕ 0
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК ЧасыСобственные,
	ВЫБОР
		КОГДА ЕСТЬNULL(тбПодр.Состояние.ЭтоПривленный, ИСТИНА) = ЛОЖЬ
			ТОГДА уатРаботаТС.Сумма + уатРаботаТС.Сумма1 + уатРаботаТС.Сумма2 + уатРаботаТС.Сумма3 + уатРаботаТС.Сумма4
		ИНАЧЕ 0
	КОНЕЦ КАК СуммаСобственные,
	ВЫБОР
		КОГДА тбПодр.Состояние = ЗНАЧЕНИЕ(Справочник.уатСостояниеТС.Привлеченный)
				ИЛИ тбПодр.Состояние ЕСТЬ NULL
				ИЛИ тбПодр.Состояние.ЭтоПривленный = ИСТИНА
			ТОГДА уатРаботаТС.Сумма + уатРаботаТС.Сумма1 + уатРаботаТС.Сумма2 + уатРаботаТС.Сумма3 + уатРаботаТС.Сумма4
		ИНАЧЕ 0
	КОНЕЦ КАК СуммаПривлеченных,
	тбПодр.Состояние
ИЗ
	РегистрНакопления.РаботаТС КАК уатРаботаТС
		ЛЕВОЕ СОЕДИНЕНИЕ ВРПодр КАК тбПодр
		ПО (тбПодр.ТС = уатРаботаТС.ТС)
			И (тбПодр.Период &lt;= уатРаботаТС.Период)
			И (тбПодр.ПериодКон &gt; уатРаботаТС.Период)
		ЛЕВОЕ СОЕДИНЕНИЕ врРее КАК регРее
		ПО (регРее.ПутевойЛист = уатРаботаТС.ПутевойЛист)
			И (регРее.ИдСтроки = уатРаботаТС.ИдСтроки)
		ЛЕВОЕ СОЕДИНЕНИЕ ВрПробегИзПутевых КАК ВрПробегИзПутевых
		ПО уатРаботаТС.ПутевойЛист = ВрПробегИзПутевых.Ссылка
			И уатРаботаТС.ИдСтроки = ВрПробегИзПутевых.ИдСтроки
ГДЕ
	НАЧАЛОПЕРИОДА(уатРаботаТС.Период, ДЕНЬ) &gt;= &amp;Дт
	И КОНЕЦПЕРИОДА(уатРаботаТС.Период, ДЕНЬ) &lt; &amp;Дт1
	И уатРаботаТС.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
	И (регРее.ПутевойЛист ЕСТЬ NULL
			ИЛИ &amp;БезРее = ЛОЖЬ)</query>
