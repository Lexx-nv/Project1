		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	уатТС.ГаражныйНомер,
	уатТС.Ссылка,
	уатТС.ГосударственныйНомер,
	уатТС.ВидМоделиТС,
	уатТС.Модель,
	уатТС.ОсновнойВидПЛ,
	уатТС.ТипТС,
	уатМестонахождениеТССрезПоследних.Подразделение,
	уатМестонахождениеТССрезПоследних.Состояние,
	1 КАК Количество,
	уатТС.ДатаВыбытия,
	уатТС.ДатаВводаВЭксплуатацию
ИЗ
	Справочник.уатТС КАК уатТС
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатМестонахождениеТС.СрезПоследних(КонецПериода(&amp;Дт,День), ) КАК уатМестонахождениеТССрезПоследних
		ПО (уатМестонахождениеТССрезПоследних.ТС = уатТС.Ссылка)
ГДЕ
	уатТС.ДатаВводаВЭксплуатацию &gt; ДАТАВРЕМЯ(1, 1, 1)
	И ((уатТС.ДатаВыбытия = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ уатТС.ДатаВыбытия &gt; &amp;Дт) или &amp;ПоказатьВыбывшие = Истина)</query>
	</dataSet>
