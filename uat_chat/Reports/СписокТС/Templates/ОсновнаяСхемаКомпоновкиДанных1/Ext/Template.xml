		<dataSource>ИсточникДанных1</dataSource>
		<query>SELECT
	Спр.Ссылка	
INTO врЮралс
FROM Справочник.уатТипыТС Спр
WHERE Спр.ссылка в иерархии(Значение(Справочник.уатТипыТС.ЮРАЛС))

;

SELECT
ТС,
Сумма(КоличествоОборот) Пробег
INTO  врПробег
FROM РегистрНакопления.уатВыработкаТС.Обороты(ДОБАВИТЬКДАТЕ(&amp;Дт,МЕсяц,-12),&amp;Дт) Рег
GROUP BY ТС

;
ВЫБРАТЬ
	уатТС.ГаражныйНомер,
	уатТС.Ссылка,
	уатТС.ГосударственныйНомер,
	уатТС.ВидМоделиТС,
	уатТС.Модель,
	уатТС.ОсновнойВидПЛ,
	уатТС.ТипТС,
	уатМестонахождениеТССрезПоследних.Подразделение,
	уатМестонахождениеТССрезПоследних.Состояние,
	уатМестонахождениеТССрезПоследних.Период ДатаСобытия,
	1 КАК Количество,
	тбПробег.Пробег КАК Пробег,
	CASE WHEN спрЮралс.ссылка IS NULL THEN "Прочие" ELSE "ЮРАЛС" END ТипЮралс,
	уатТС.ДатаВыбытия,
	ТарифыТСнаЗПСрезПоследних.Линия Линия,
	ТарифыТСнаЗПСрезПоследних.ЛинияПов ЛинияПов,
	ТарифыТСнаЗПСрезПоследних.Разрыв Разрыв,
	ТарифыТСнаЗПСрезПоследних.Ремонт Ремонт,
	уатТС.ДатаВводаВЭксплуатацию
ИЗ
	Справочник.уатТС КАК уатТС
		INNER JOIN РегистрСведений.уатМестонахождениеТС.СрезПоследних(&amp;Дт,  ) КАК уатМестонахождениеТССрезПоследних
		ПО (уатМестонахождениеТССрезПоследних.ТС = уатТС.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ТарифыТСнаЗП.СрезПоследних(&amp;Дт,  ) КАК ТарифыТСнаЗПСрезПоследних
		ПО (ТарифыТСнаЗПСрезПоследних.Модель = уатТС.Модель)
		LEFT OUTER JOIN врЮралс СпрЮралс ON СпрЮралс.ссылка = уатТС.ТипТС
		LEFT OUTER JOIN врПробег тбПробег ON тбПробег.ТС = уатТС.Ссылка
		
ГДЕ
	уатТС.ДатаВводаВЭксплуатацию &gt; ДАТАВРЕМЯ(1, 1, 1)
	И уатТС.ДатаВводаВЭксплуатацию &lt;= &amp;Дт
	И ((уатТС.ДатаВыбытия = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ уатТС.ДатаВыбытия &gt; &amp;Дт) или &amp;ПоказатьВыбывшие = Истина)</query>
	</dataSet>
