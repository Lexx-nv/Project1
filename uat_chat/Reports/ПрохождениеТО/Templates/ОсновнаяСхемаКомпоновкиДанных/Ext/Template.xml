		<dataSource>ИсточникДанных1</dataSource>
		<query>SELECT
	уатВыработкаТС.ТС,
	СУММА(уатВыработкаТС.Количество)+ ТС.НачальныйПробег КАК Пробег
INTO врИтПрб
FROM  РегистрНакопления.уатВыработкаТС КАК уатВыработкаТС
СГРУППИРОВАТЬ ПО
	уатВыработкаТС.ТС
;
//////////////////////////////////////////
SELECT
  ТС,
  СУММА(Пробег)/ (Количество(Дт)*7) срДень,
  СУММА(Пробег)/ (Количество(Дт))   срНеделя
INTO врСрПрб
FROM 
(
SELECT
	уатВыработкаТС.ТС,
	НачалоПериода(Период,Неделя) Дт,
	СУММА(уатВыработкаТС.Количество) КАК Пробег
FROM  РегистрНакопления.уатВыработкаТС КАК уатВыработкаТС
WHERE  Период &gt;= ДобавитьКДате(&amp;ДтПериод,Месяц,-3) и Период &lt; &amp;ДтПериод
   и   Количество &lt;&gt; 0
СГРУППИРОВАТЬ ПО
	уатВыработкаТС.ТС,
	НачалоПериода(Период,Неделя)
) WWW
GROUP BY ТС	
;
//////////////////////////////////////////
ВЫБРАТЬ
	уатТС.ссылка ТС,
	уатТС.Гаражныйномер тсГаражныйномер,
	уатТС.ГосударственныйНомер тсГосударственныйНомер,
	уатМоделиТСНормативыОбслуживания.ВидОбслуживания ВидТО,
	уатПрохождениеТОСрезПоследних.Период,
	СУММА(ЕСТЬNULL(уатВыработкаТС.Количество, 0)) КАК Количество,
	уатМоделиТСНормативыОбслуживания.ЗначениеПараметраВыработки КАК Норма,
		CASE WHEN ISNULL(ЗначениеПараметраВыработки,0)&lt;&gt;0 THEN СУММА(ISNULL(уатВыработкаТС.Количество,0))/ЗначениеПараметраВыработки*100 ELSE 0 END процТО,
	уатМестонахождениеТССрезПоследних.Подразделение,
	ИтПрб.Пробег итПробег,
	срПрб.СрДень,
	срПрб.СрНеделя,
	CASE WHEN ЕСТЬNULL(уатМоделиТСНормативыОбслуживания.ЗначениеПараметраВыработки,0)-СУММА(ЕСТЬNULL(уатВыработкаТС.Количество, 0)) &lt;= 0 и срПрб.СрНеделя&gt;0
	     THEN НачалоПериода(&amp;ДтПериод,Неделя) 
	     WHEN срПрб.СрДень&gt;0 и (ЕСТЬNULL(уатМоделиТСНормативыОбслуживания.ЗначениеПараметраВыработки,0)-СУММА(ЕСТЬNULL(уатВыработкаТС.Количество, 0))) / срПрб.СрДень &lt; 50
	     THEN НачалоПериода( ДобавитьКДате(&amp;ДтПериод,День,(ЕСТЬNULL(уатМоделиТСНормативыОбслуживания.ЗначениеПараметраВыработки,0)-СУММА(ЕСТЬNULL(уатВыработкаТС.Количество, 0))) / срПрб.СрДень) , Неделя) 
	     ELSE ДатаВремя(2030,1,1,0,0,0) END ДтПлан
	     
//ЭтуСтрокуМожноЗаменить	
ИЗ
    Справочник.уатТс как уатТС

		inner join Справочник.уатМоделиТС.НормативыОбслуживания КАК уатМоделиТСНормативыОбслуживания
		ПО уатТС.Модель = уатМоделиТСНормативыОбслуживания.Ссылка
		
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатПрохождениеТО.СрезПоследних(&amp;ДтПериод) КАК уатПрохождениеТОСрезПоследних
		по уатПрохождениеТОСрезПоследних.тс = уаттс.ссылка
			И уатПрохождениеТОСрезПоследних.ВидТО = уатМоделиТСНормативыОбслуживания.ВидОбслуживания
			
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.уатВыработкаТС КАК уатВыработкаТС
		ПО уатТС.ссылка = уатВыработкаТС.ТС
			И ISNULL(уатПрохождениеТОСрезПоследних.Период,Датавремя(1,1,1,0,0,0)) &lt; уатВыработкаТС.Период
			И (&amp;ДтПериод &gt;= уатВыработкаТС.Период)
			
			
		inner join РегистрСведений.уатМестонахождениеТС.СрезПоследних(&amp;ДтПериод) КАК уатМестонахождениеТССрезПоследних
		ПО уатТС.ссылка = уатМестонахождениеТССрезПоследних.ТС
		
		ЛЕВОЕ СОЕДИНЕНИЕ врИтПрб КАК ИтПрб
		ПО уатТС.ссылка = ИтПрб.ТС
		
		ЛЕВОЕ СОЕДИНЕНИЕ врСрПрб КАК срПрб
		ПО уатТС.ссылка = срПрб.ТС
		
WHERE  
уатМестонахождениеТССрезПоследних.состояние.ВидСостояния = Значение(Перечисление.уатВидыСостоянийТС.врейсе) 

СГРУППИРОВАТЬ ПО
	уатТС.ссылка,
	уатМоделиТСНормативыОбслуживания.ВидОбслуживания,
	уатПрохождениеТОСрезПоследних.Период,
	уатМоделиТСНормативыОбслуживания.ЗначениеПараметраВыработки,
	уатМестонахождениеТССрезПоследних.Подразделение,
	ИтПрб.Пробег,
	срПрб.СрДень,
	срПрб.СрНеделя 

</query>
