		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
    уатМестонахождениеТССрезПоследних.Подразделение,
    уатМестонахождениеТССрезПоследних.Состояние,
	уатОстаткиГСМнаТСОстаткиИОбороты.ГСМ,
	уатОстаткиГСМнаТСОстаткиИОбороты.ТС,
	уатОстаткиГСМнаТСОстаткиИОбороты.ТС.ГаражныйНомер,
	уатОстаткиГСМнаТСОстаткиИОбороты.ТС.ГосударственныйНомер,
	уатОстаткиГСМнаТСОстаткиИОбороты.Партия,
	уатОстаткиГСМнаТСОстаткиИОбороты.ДокПартии,
	уатОстаткиГСМнаТСОстаткиИОбороты.БакОсновной,
	уатОстаткиГСМнаТСОстаткиИОбороты.КоличествоНачальныйОстаток,
	уатОстаткиГСМнаТСОстаткиИОбороты.КоличествоПриход,
	уатОстаткиГСМнаТСОстаткиИОбороты.КоличествоРасход,
	уатОстаткиГСМнаТСОстаткиИОбороты.КоличествоКонечныйОстаток,
	0 СливПрх,
	0 СливРсхПЛ,
	0 СливРсхСкл
ИЗ
	РегистрНакопления.уатОстаткиГСМнаТС.ОстаткиИОбороты КАК уатОстаткиГСМнаТСОстаткиИОбороты
ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатМестонахождениеТС.СрезПоследних(, ) КАК уатМестонахождениеТССрезПоследних
	ПО (уатМестонахождениеТССрезПоследних.ТС = уатОстаткиГСМнаТСОстаткиИОбороты.ТС)
	
	
UNION ALL

ВЫБРАТЬ
    уатМестонахождениеТССрезПоследних.Подразделение,
    уатМестонахождениеТССрезПоследних.Состояние,
	уатОстаткиГСМнаТС.ГСМ,
	уатОстаткиГСМнаТС.ТС,
	уатОстаткиГСМнаТС.ТС.ГаражныйНомер ГарНомер,
	уатОстаткиГСМнаТС.ТС.ГосударственныйНомер ГосНомер,
	уатОстаткиГСМнаТС.Партия,
	уатОстаткиГСМнаТС.ДокПартии,
	уатОстаткиГСМнаТС.БакОсновной,
	0,
	CASE WHEN ВидДвижения = Значение(ВидДвиженияНакопления.Приход) THEN -Количество ELSE 0 END,
	CASE WHEN ВидДвижения = Значение(ВидДвиженияНакопления.Расход) THEN -Количество ELSE 0 END,
	0,
	CASE WHEN ВидДвижения = Значение(ВидДвиженияНакопления.Приход) THEN Количество ELSE 0 END СливПрх,
	CASE WHEN ВидДвижения = Значение(ВидДвиженияНакопления.Расход) и Слив = 1 THEN Количество ELSE 0 END СливРсхПЛ,
	CASE WHEN ВидДвижения = Значение(ВидДвиженияНакопления.Расход) и Слив = 2 THEN Количество ELSE 0 END СливРсхСкл
ИЗ
	РегистрНакопления.уатОстаткиГСМнаТС КАК уатОстаткиГСМнаТС
ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатМестонахождениеТС.СрезПоследних(, ) КАК уатМестонахождениеТССрезПоследних
	ПО (уатМестонахождениеТССрезПоследних.ТС = уатОстаткиГСМнаТС.ТС)

	
	
WHERE Слив &lt;&gt; 0 
и уатОстаткиГСМнаТС.Период &gt;= &amp;НачалоПериода
и уатОстаткиГСМнаТС.Период &lt;= &amp;КонецПериода</query>
