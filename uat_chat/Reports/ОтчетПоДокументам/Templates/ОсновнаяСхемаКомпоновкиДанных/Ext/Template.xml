		<query>
SELECT 
	  ТС,уатДокументыТС.ВидДокумента,  CASE WHEN ДатаОкончания=ДатаВремя(1,1,1) THEN ДатаОкончания ELSE NULL END дтКон,  MAX(ДатаОкончания) дтМакс
INTO врУатДокументыТС0	   
FROM РегистрСведений.уатДокументыТС КАК уатДокументыТС
LEFT OUTER JOIN  РегистрСведений.ДокументыИсключения.СрезПоследних(,ВидДокумента = &amp;ВидДок или &amp;ВидДок = Значение(Справочник.уатВидыДДД.ПустаяСсылка)) РегИскл ON РегИскл.ВидДОкумента = уатДокументыТС.ВидДОкумента и РегИскл.ОбъектДок = уатДокументыТС.ТС и РегИскл.Исключить = Истина

LEFT OUTER JOIN Справочник.уатВидыДДД.ИсключаемыеОбъектыКонтроля исклСпр ON исклСпр.ОбъектКонтроля = ТС


WHERE (уатДокументыТС.ВидДокумента = &amp;ВидДок или &amp;ВидДок = Значение(Справочник.уатВидыДДД.ПустаяСсылка))
    и РегИскл.ОбъектДок IS NULL
    и исклСпр.ОбъектКонтроля IS NULL
    и (ТС = &amp;фильтрТССотр or &amp;ЕстьфильтрТССотр = ложь )
GROUP BY  ТС,уатДокументыТС.ВидДокумента,CASE WHEN ДатаОкончания=ДатаВремя(1,1,1) THEN ДатаОкончания ELSE NULL END
;

SELECT
	уатДокументыТС.ТС,
	уатДокументыТС.ВидДокумента, 
	уатДокументыТС.Комментарий Как Комментарий, 
	Значение(Справочник.Контрагенты.ПустаяСсылка) Контрагент,
	MAX(уатДокументыТС.Серия) Серия, 
	MAX(уатДокументыТС.Номер) Номер, 
	MAX(ДатаВыдачи) ДатаВыдачи, 
	MAX(ДатаОкончания) ДатаОкончания,
	РАЗНОСТЬДАТ(&amp;ДТ, CASE WHEN  MAX(ДатаОкончания) = ДатаВремя(1,1,1) THEN ДатаВремя(2050,1,1) ELSE MAX(ДатаОкончания) END, ДЕНЬ) КАК Дней
	
INTO врУатДокументыТС1
FROM РегистрСведений.уатДокументыТС КАК уатДокументыТС

INNER JOIN врУатДокументыТС0 ТБл ON Тбл.ТС = уатДокументыТС.ТС
								  и Тбл.ВидДокумента = уатДокументыТС.ВидДокумента
								  и ISNULL(Тбл.дтКон,Тбл.дтМакс) = уатДокументыТС.ДатаОкончания
								   
GROUP BY
уатДокументыТС.ТС,
уатДокументыТС.ВидДокумента,
уатДокументыТС.Комментарий

//Добавим топливные карты
UNION ALL

SELECT
ТопливныеКартыСрезПоследних.ТС,
Значение(Справочник.уатВидыДДД.ТопливныеКарты) ВидДокумента,
"" Комментарий,
СпрКарт.КемВыдана Контрагент,
"" Серия,
СпрКарт.Код Номер,
ТопливныеКартыСрезПоследних.Период ДатаВыдачи,
ДатаВремя(1,1,1) ДатаОкончания,
РАЗНОСТЬДАТ(&amp;ДТ, CASE WHEN  ДатаОкончания = ДатаВремя(1,1,1) THEN ДатаВремя(2050,1,1) ELSE ДатаОкончания END, ДЕНЬ) КАК Дней

FROM РегистрСведений.ТопливныеКарты.СрезПоследних КАК ТопливныеКартыСрезПоследних
INNER JOIN Справочник.уатПластиковыеКарты спрКарт ON СпрКАрт.ссылка = ТопливныеКартыСрезПоследних.Карта
WHERE ТопливныеКартыСрезПоследних.ТС &lt;&gt; Значение(Справочник.уатТС.ПустаяСсылка)

;

SELECT
	спрТС.Ссылка ТС,
	спрТС.Наименование имяТС,
	ISNULL(уатДокументыТС.ВидДокумента,СпрДДД.ссылка) ВидДокумента, 
	ISNULL(уатДокументыТС.Контрагент,Значение(Справочник.Контрагенты.ПустаяСсылка)) Контрагент,
	ISNULL(уатДокументыТС.Комментарий,"") Комментарий,
	уатДокументыТС.Серия, 
	уатДокументыТС.Номер, 
	спрТС.ГаражныйНомер,
	спрТС.ГосударственныйНомер,
	уатМестонахождениеТССрезПоследних.Состояние,
	уатМестонахождениеТССрезПоследних.Подразделение,
	уатДокументыТС.ДатаВыдачи, 
	уатДокументыТС.ДатаОкончания,
	ISNULL(уатДокументыТС.Дней,0) Дней 
	
INTO врУатДокументыТС
FROM Справочник.уатТС спрТС 
	                              
ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.уатМестонахождениеТС.СрезПоследних КАК уатМестонахождениеТССрезПоследних
		ПО спрТС.Ссылка = уатМестонахождениеТССрезПоследних.ТС
		 и уатМестонахождениеТССрезПоследних.Состояние.ЗапретитьВыпискуПЛ = Ложь 

INNER JOIN Справочник.уатВидыДДД СпрДДД ON  СпрДДД.ДляТС = Истина 
                                         и (СпрДДД.ссылка = &amp;ВидДок или &amp;ВидДок = Значение(Справочник.уатВидыДДД.ПустаяСсылка))
	                              
LEFT OUTER JOIN врУатДокументыТС1 КАК уатДокументыТС ON уатДокументыТС.ТС = спрТС.ссылка 
                                                      и уатДокументыТС.ВидДокумента = СпрДДД.ссылка

LEFT OUTER JOIN Справочник.уатВидыДДД.ПодразделенияФильтр СпрПодрФ ON СпрПодрФ.Ссылка        = СпрДДД.ссылка
                                                                    и СпрПодрФ.Подразделение = уатМестонахождениеТССрезПоследних.Подразделение

LEFT OUTER JOIN Справочник.уатВидыДДД.ИсключаемыеОбъектыКонтроля исклСпр ON исклСпр.ОбъектКонтроля = спрТС.ссылка


WHERE (спрТС.ДатаВыбытия &gt;= &amp;ДТ или спрТС.ДатаВыбытия = ДатаВремя(1,1,1)  )
и ( СпрДДД.ОбязательныйДляВсех = Истина OR уатДокументыТС.ТС IS NOT NULL  )
и (уатМестонахождениеТССрезПоследних.Состояние&lt;&gt;Значение(Справочник.уатСостояниеТС.Привлеченный) или уатДокументыТС.ТС IS NOT NULL)

и (  (СпрДДД.ОбязательноеПодразделениеЕсть = Истина и СпрПодрФ.Исключить = Ложь) or СпрДДД.ОбязательноеПодразделениеЕсть = FALSE или уатДокументыТС.ТС IS NOT NULL )
и ISNULL(СпрПодрФ.Исключить,FALSE) = FALSE
 	
и  исклСпр.ОбъектКонтроля IS NULL

и (спрТС.Ссылка = &amp;фильтрТССотр or &amp;ЕстьфильтрТССотр = ложь )
;
//======С0трудники=========================

SELECT 
	  Сотрудник ,уатДокументыВодителей.ВидДокумента,  CASE WHEN ДатаОкончания=ДатаВремя(1,1,1) THEN ДатаОкончания ELSE NULL END дтКон,  MAX(ДатаОкончания) дтМакс
INTO врУатДокументыСотр0	   
FROM РегистрСведений.уатДокументыВодителей КАК уатДокументыВодителей
LEFT OUTER JOIN  РегистрСведений.ДокументыИсключения.СрезПоследних(,ВидДокумента = &amp;ВидДок или &amp;ВидДок = Значение(Справочник.уатВидыДДД.ПустаяСсылка)) РегИскл ON РегИскл.ВидДОкумента = уатДокументыВодителей.ВидДОкумента и РегИскл.ОбъектДок = уатДокументыВодителей.Сотрудник и РегИскл.Исключить = Истина

LEFT OUTER JOIN Справочник.уатВидыДДД.ИсключаемыеОбъектыКонтроля исклСпр ON исклСпр.ОбъектКонтроля = Сотрудник

WHERE (уатДокументыВодителей.ВидДокумента = &amp;ВидДок или &amp;ВидДок = Значение(Справочник.уатВидыДДД.ПустаяСсылка))
    и РегИскл.ОбъектДок IS NULL
    и исклСпр.ОбъектКонтроля IS NULL
    и (Сотрудник = &amp;фильтрТССотр or &amp;ЕстьфильтрТССотр = ложь)
GROUP BY  Сотрудник,уатДокументыВодителей.ВидДокумента,CASE WHEN ДатаОкончания=ДатаВремя(1,1,1) THEN ДатаОкончания ELSE NULL END
;

SELECT
	уатДокументыВодителей.Сотрудник ТС,
	уатДокументыВодителей.ВидДокумента,
	уатДокументыВодителей.Комментарий,
	Значение(Справочник.Контрагенты.ПустаяСсылка) Контрагент, 
	MAX(уатДокументыВодителей.Серия) Серия, 
	MAX(уатДокументыВодителей.Номер) Номер, 
	MAX(ДатаВыдачи) ДатаВыдачи, 
	MAX(ДатаОкончания) ДатаОкончания,
	РАЗНОСТЬДАТ(&amp;ДТ, CASE WHEN  MAX(ДатаОкончания) = ДатаВремя(1,1,1) THEN ДатаВремя(2050,1,1) ELSE MAX(ДатаОкончания) END, ДЕНЬ) КАК Дней
	
INTO врУатДокументыСотр1
FROM РегистрСведений.уатДокументыВодителей КАК уатДокументыВодителей

INNER JOIN врУатДокументыСотр0 ТБл ON Тбл.Сотрудник = уатДокументыВодителей.Сотрудник
								  и Тбл.ВидДокумента = уатДокументыВодителей.ВидДокумента
								  и ISNULL(Тбл.дтКон,Тбл.дтМакс) = уатДокументыВодителей.ДатаОкончания
								   
	
GROUP BY 	
    уатДокументыВодителей.Сотрудник,
	уатДокументыВодителей.ВидДокумента, 
    уатДокументыВодителей.Комментарий

//Добавим топливные карты
UNION ALL

SELECT
ТопливныеКартыСрезПоследних.Сотрудник ТС,
Значение(Справочник.уатВидыДДД.ТопливныеКарты) ВидДокумента,
"" Комментарий,
СпрКарт.КемВыдана Контрагент,
"" Серия,
СпрКарт.Код Номер,
ТопливныеКартыСрезПоследних.Период ДатаВыдачи,
ДатаВремя(1,1,1) ДатаОкончания,
РАЗНОСТЬДАТ(&amp;ДТ, CASE WHEN  ДатаОкончания = ДатаВремя(1,1,1) THEN ДатаВремя(2050,1,1) ELSE ДатаОкончания END, ДЕНЬ) КАК Дней

FROM РегистрСведений.ТопливныеКарты.СрезПоследних КАК ТопливныеКартыСрезПоследних
INNER JOIN Справочник.уатПластиковыеКарты спрКарт ON СпрКАрт.ссылка = ТопливныеКартыСрезПоследних.Карта
WHERE ТопливныеКартыСрезПоследних.Сотрудник &lt;&gt; Значение(Справочник.уатСотрудники.ПустаяСсылка)


;

SELECT
	спрСотр.ссылка ТС,
	спрСотр.Наименование ИмяТС,
	ISNULL(уатДокументыВодителей.ВидДокумента,СпрДДД.ссылка) ВидДокумента,
	ISNULL(уатДокументыВодителей.Контрагент,Значение(Справочник.Контрагенты.ПустаяСсылка)) Контрагент, 
	ISNULL(уатДокументыВодителей.Комментарий,"") Комментарий, 
	уатДокументыВодителей.Серия, 
	уатДокументыВодителей.Номер, 
	спрСотр.Код ГаражныйНомер,
	"" ГосударственныйНомер,
	уатСведенияОСотрудниках.Должность Состояние,
	уатСведенияОСотрудниках.ПодразделениеОрганизации Подразделение,
	уатДокументыВодителей.ДатаВыдачи, 
	уатДокументыВодителей.ДатаОкончания,
	ISNULL(уатДокументыВодителей.Дней,0) Дней
	
INTO врУатДокументыСотр
FROM Справочник.уатСотрудники спрСотр 
	                              
INNER JOIN РегистрСведений.уатСведенияОСотрудниках.СрезПоследних КАК уатСведенияОСотрудниках ПО спрСотр.ссылка = уатСведенияОСотрудниках.Сотрудник
																							  и уатСведенияОСотрудниках.ПодразделениеОрганизации &lt;&gt; Значение(Справочник.ПодразделенияОрганизаций.ПустаяСсылка) 

INNER JOIN Справочник.уатВидыДДД СпрДДД ON  СпрДДД.ДляСотр = Истина 
                                         и (СпрДДД.ссылка = &amp;ВидДок или &amp;ВидДок = Значение(Справочник.уатВидыДДД.ПустаяСсылка))
                                              
LEFT OUTER JOIN врУатДокументыСотр1 КАК уатДокументыВодителей  ON уатДокументыВодителей.ТС = спрСотр.ссылка
                                                                и уатДокументыВодителей.ВидДокумента = СпрДДД.ссылка

LEFT OUTER JOIN Справочник.уатВидыДДД.ПодразделенияФильтр СпрПодрФ ON СпрПодрФ.Ссылка        = СпрДДД.ссылка
                                                                    и СпрПодрФ.Подразделение = уатСведенияОСотрудниках.ПодразделениеОрганизации

LEFT OUTER JOIN Справочник.уатВидыДДД.ИсключаемыеОбъектыКонтроля исклСпр ON исклСпр.ОбъектКонтроля = спрСотр.ссылка


WHERE (спрСотр.ДатаУвольнения &gt;= &amp;ДТ или спрСотр.ДатаУвольнения = ДатаВремя(1,1,1)  )								   
и ( СпрДДД.ОбязательныйДляВсех = Истина OR уатДокументыВодителей.ТС IS NOT NULL  )

и (  (СпрДДД.ОбязательноеПодразделениеЕсть = Истина и СпрПодрФ.Исключить = Ложь) or СпрДДД.ОбязательноеПодразделениеЕсть = FALSE OR уатДокументыВодителей.ТС IS NOT NULL )
и ISNULL(СпрПодрФ.Исключить,FALSE) = FALSE

и  исклСпр.ОбъектКонтроля IS NULL
 	
и (спрСотр.Ссылка = &amp;фильтрТССотр or &amp;ЕстьфильтрТССотр = ложь)
;
ВЫБРАТЬ
	уатДокументыТС.ГаражныйНомер,
	уатДокументыТС.ГосударственныйНомер,
	уатДокументыТС.имяТС,
	уатДокументыТС.ТС,
	СпрДДД.Родитель Группа,
	СпрДДД.стрОтветсвенные Ответсвенные,
	уатДокументыТС.ВидДокумента,
	уатДокументыТС.Комментарий,
	уатДокументыТС.Контрагент,
	уатДокументыТС.Серия+уатДокументыТС.Номер Номер,
	уатДокументыТС.ДатаВыдачи,
	уатДокументыТС.ДатаОкончания,
	уатДокументыТС.Дней,
	CASE WHEN Дней &lt; 5  THEN "Критический" 
	     WHEN Дней &lt; 30 THEN "Заканчивается"
	                    ELSE "Норма" END Инд,   
	"--ТС--" Категория1,        
	уатДокументыТС.Состояние,
	уатДокументыТС.Подразделение
ИЗ
	врУатДокументыТС КАК уатДокументыТС
LEFT OUTER JOIN Справочник.уатВидыДДД СпрДДД ON СпрДДД.ссылка = уатДокументыТС.ВидДокумента
WHERE (Дней &lt;= &amp;Дней или &amp;Дней = 0)
    и (уатДокументыТС.Подразделение в иерархии(&amp;Подр) или &amp;Подр = Значение(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)) 
 
     
   
UNION ALL

ВЫБРАТЬ
	уатДокументыТС.ГаражныйНомер,
	уатДокументыТС.ГосударственныйНомер,
	уатДокументыТС.имяТС,
	уатДокументыТС.ТС,
	СпрДДД.Родитель Группа,
	СпрДДД.стрОтветсвенные Ответсвенные,
	уатДокументыТС.ВидДокумента,
	уатДокументыТС.Комментарий,
	уатДокументыТС.Контрагент,
	уатДокументыТС.Серия+уатДокументыТС.Номер Номер,
	уатДокументыТС.ДатаВыдачи,
	уатДокументыТС.ДатаОкончания,
	уатДокументыТС.Дней,
	CASE WHEN Дней &lt; 5  THEN "Критический" 
	     WHEN Дней &lt; 30 THEN "Заканчивается"
	                    ELSE "Норма" END Инд,   
	        
	"--Сотрудники--" Категория1,        
	уатДокументыТС.Состояние,
	уатДокументыТС.Подразделение
ИЗ
	врУатДокументыСотр КАК уатДокументыТС
LEFT OUTER JOIN Справочник.уатВидыДДД СпрДДД ON СпрДДД.ссылка = уатДокументыТС.ВидДокумента
WHERE (Дней &lt;= &amp;Дней или &amp;Дней = 0) 
    и (уатДокументыТС.Подразделение в иерархии(&amp;Подр) или &amp;Подр = Значение(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)) 
    и (уатДокументыТС.ВидДокумента.ПометкаУдаления = Ложь) 
	
	
	
</query>
