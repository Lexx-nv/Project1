		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	уатСливГСМ.Ссылка,
	уатСливГСМ.ТС,
	уатСливГСМ.Количество,
	уатСливГСМ.Количество1,
	CASE WHEN уатСливГСМ.ПутевойЛист = Значение(Документ.уатПутевойЛист.ПустаяСсылка) THEN АЗС ELSE ПутевойЛист END ПутевойЛист,
	уатСливГСМ.ПутевойЛист.ТранспортноеСредство,
	уатСливГСМ.ПутевойЛист.ТранспортноеСредство.ГаражныйНомер,
	уатСливГСМ.ТС.ГаражныйНомер
ИЗ
	Документ.уатСливГСМ КАК уатСливГСМ
ГДЕ
	уатСливГСМ.Проведен = Истина
	И уатСливГСМ.Дата &gt;= &amp;Дт
	И уатСливГСМ.Дата &lt; &amp;Дт1</query>
